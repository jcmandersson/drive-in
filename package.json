  "devDependencies": {
    "babel": "^4.6.6",
    "babel-eslint": "^1.0.14",
    "babelify": "^5.0.4",
    "browserify": "^9.0.3",
    "coveralls": "^2.11.2",
    "eslint": "^0.16.1",
